<div id="products" class="row justify-content-center my-4">
    <% if (info) {  %>
        <% for (const [key, product] of Object.entries(info)) { %>
            <div class="col m-3">
                <div class="card h-100 shadow-sm rounded p-3">
                    <div class="card-body ">
                        <div class="text-center">
                            <h2> Name: <%= product.product %></h2>
                            <h3>Amout: <%= product.amout %></h3>
                        </div>
                        <img class="card-img-top p-1" src="<%= product.productUrl %> " alt="">
                    </div>
                    
                    <div class="card-footer p-4 pt-0 border-top-0 bg-transparent">
                        <div class="text-center"><a class="btn btn-outline-dark mt-auto" href="#">Add to cart</a></div>
                    </div>
                </div>
            </div>    
        <% } %>
    <% } else { %>
        <p>Oops something went wrong</p>
    <% } %>
</div>